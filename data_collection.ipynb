    "def clean_prices(df):\n",
    "    # Rename 'Unnamed: 0' to 'asin'\n",
    "    df = df.rename(columns={'Unnamed: 0': 'asin'})\n",
    "    \n",
    "    # Round every value in all columns except the first (asin) to 2 decimal places\n",
    "    df.iloc[:, 1:] = df.iloc[:, 1:].round(2)\n",
    "    \n",
    "    return df"
   ]
